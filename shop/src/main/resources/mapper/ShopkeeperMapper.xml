<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shop.mapper.ShopkeeperMapper">
    <resultMap id="BaseResultMap" type="com.shop.entity.Shopkeeper">
        <result column="id" jdbcType="INTEGER" property="id"></result>
        <result column="keepername" jdbcType="VARCHAR" property="keepername"></result>
        <result column="password" jdbcType="VARCHAR" property="password"></result>
        <result column="exchange_id" jdbcType="INTEGER" property="exchange_id"></result>
        <result column="buyerid" jdbcType="INTEGER" property="buyerid"></result>
        <result column="keepercount" jdbcType="INTEGER" property="keepercount"></result>
        <result column="exchange_time" jdbcType="DATE" property="exchange_time"></result>

    </resultMap>

    <select id="selectAll" resultMap="BaseResultMap">
        select * from shopkeeper
    </select>

    <insert id="insert"  parameterType="com.shop.entity.Shopkeeper">
        insert  into shopkeeper (id, exchange_id,buyerid, keepercount, exchange_time, keepername  )
        values(#{id}, #{exchange_id}, #{buyerid},#{keepercount},#{exchange_time},#{keepername} )



    </insert>

</mapper>