<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shop.mapper.Shopkeeper_accountMapper">
    <select id="getAll" resultType="com.shop.entity.Shopkeeper_account">
        select * from goods
    </select>
    <select id="getByName" parameterType="String" resultType="com.shop.entity.Shopkeeper_account">
        select * from shopkeeper_account where name =#{name}

    </select>


    <update id="update_moneyById" parameterType="Map" >
        update shopkeeper_account set money = #{money} where id =#{id}
    </update>
    
    <update id="update_moneyByName" parameterType="Map">
        update shopkeeper_account set money = #{money} where name =#{name}

    </update>



</mapper>